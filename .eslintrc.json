{
  "parser": "babel-eslint",
  "extends": ["airbnb/base", "plugin:import/errors"],
  "rules": {
    "no-use-before-define": 0,
    "arrow-body-style": 0,
    "dot-notation": 0,
    "no-console": 0,
    "comma-dangle": ["error", "never"],
    "eol-last": 0,
    "import/extensions": 0,
    "import/no-unresolved": 0,
    "no-underscore-dangle": 0,
    "no-undef": 0,
    "arrow-parens": 0,
    "no-eval": 0,
    "unused-vars": 0
  }
}

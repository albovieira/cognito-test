    "express": "^4.16.3",
    "express-session": "^1.15.6",
    "jsonwebtoken": "^8.3.0",
    "jwk-to-pem": "^2.0.0",
    "jwt-decode": "^2.2.0",
    "lodash": "^4.17.10",
